<?xml version="1.0" encoding="UTF-8"?>
<!-- This web.xml file is not required when using Servlet 3.0 container,
     see implementation details http://jersey.java.net/nonav/documentation/latest/jax-rs.html -->
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>Jersey Web Application</servlet-name>
    <servlet-class>org.glassfish.jersey.servlet.ServletContainer</servlet-class>
    <init-param>
      <param-name>jersey.config.server.provider.packages</param-name>
      <param-value>com.coupon</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Jersey Web Application</servlet-name>
    <url-pattern>/webapi/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>rcservlet</servlet-name>
    <servlet-class>com.coupon.ReadCouponServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>rcservlet</servlet-name>
    <url-pattern>/rcservlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>cpservlet</servlet-name>
    <servlet-class>com.coupon.ComputePrice2</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>cpservlet</servlet-name>
    <url-pattern>/cpservlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CP</servlet-name>
    <servlet-class>couponModel.ComputePrice</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CP</servlet-name>
    <url-pattern>/CP</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CPrc</servlet-name>
    <servlet-class>couponModel.Cprice</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CPrc</servlet-name>
    <url-pattern>/CPrc</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ValidationCheck</servlet-name>
    <servlet-class>couponModel.ValidationCheck</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ValidationCheck</servlet-name>
    <url-pattern>/ValidationCheck</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AvailableCoupons</display-name>
    <servlet-name>AvailableCoupons</servlet-name>
    <servlet-class>couponModel.AvailableCoupons</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AvailableCoupons</servlet-name>
    <url-pattern>/AvailableCoupons</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>UploadCouponPackage</display-name>
    <servlet-name>UploadCouponPackage</servlet-name>
    <servlet-class>couponModel.UploadCouponPackage</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploadCouponPackage</servlet-name>
    <url-pattern>/UploadCouponPackage</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>login</display-name>
    <servlet-name>login</servlet-name>
    <servlet-class>couponModel.login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
</web-app>